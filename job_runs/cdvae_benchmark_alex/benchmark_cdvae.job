python write_benchmark.py \
    --run_dir ./hydra_outputs/singlerun/????-??-??/alexandria \
    --use_tensor recon \
    --data_dir ../../models/cdvae/data/alexandria \
    --split    test \
    --output_csv AI-AtomGen-prop-dft_3d-test-rmse.csv \
    --dump_json alexandria_cdvae.json

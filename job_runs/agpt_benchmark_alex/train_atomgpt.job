#!/bin/bash

#SBATCH --job-name=agpt_alex
#SBATCH --nodes=1                         
#SBATCH --gres=gpu:1
#SBATCH --ntasks-per-node=1             
#SBATCH --cpus-per-task=8
#SBATCH --time=08:00:00                    # Max walltime (HH:MM:SS)
#SBATCH --mem=8G                           # Total RAM for the job (8 GB)

source ~/.bashrc
module load cuda/11.8
eval "$(conda shell.bash hook)"
conda activate my_atomgpt
nvidia-smi

python models/atomgpt/atomgpt/inverse_models/inverse_models.py \
    --config_name job_runs/agpt_benchmark_alex/config.json

mv AI* job_runs/agpt_benchmark_alex/
